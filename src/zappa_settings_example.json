        "aws_region": "$ZAPPA_AWS_REGION",
        "django_settings": "shiro.settings",
        "profile_name": "codear",
        "project_name": "shiro",
        "runtime": "python3.7",
        "s3_bucket": "$ZAPPA_BUCKET_NAME",
